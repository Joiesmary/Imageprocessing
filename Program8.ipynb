   "source": [
    "import cv2\n",
    "img=cv2.imread('plant.jpeg')\n",
    "cv2.imshow(\"RGB\",img)\n",
    "cv2.waitKey(0)\n",
    "img=cv2.imread('plant.jpeg',0)\n",
    "cv2.imshow(\"Gray\",img)\n",
    "cv2.waitKey(0)\n",
    "ret,bw_img=cv2.threshold(img,127,255,cv2.THRESH_BINARY)\n",
    "cv2.imshow(\"Binary\",bw_img)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
